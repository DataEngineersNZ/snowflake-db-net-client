<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>0.4.0</Version>
    <Authors>fixer_m, jonhopper-dataengineers</Authors>
    <Description>Unofficial .NET client for Snowflake REST API. 
Provides straightforward and efficient way to execute SQL queries in Snowflake and automatically map response to your models.
    </Description>
    <Copyright>Copyright (c) 2020-2021 Ilya Bystrov, 2021 Data Engineers Ltd</Copyright>
    <PackageProjectUrl>https://github.com/DataEngineersNZ/snowflake-db-net-client/</PackageProjectUrl>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/DataEngineersNZ/snowflake-db-net-client/</RepositoryUrl>
    <PackageTags>snowflake;client;api;wrapper;snowflakedb;rest;restapi</PackageTags>
  </PropertyGroup>

</Project>
